{
    "name": "rene-roscher/pterodactyl-api-addon",
    "description": "This package brings an API interface to the application API in order to be able to create API tokens for a user",
    "keywords": [
        "PterodactylApiAddon",
        "laravel",
        "pterodactyl-api-addon"
    ],
    "homepage": "https://github.com/pterodactylapiaddon/pterodactyl-api-addon",
    "license": "MIT",
    "authors": [
        {
            "name": "Rene Roscher",
            "email": "roscher794@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Xepare\\PterodactylApiAddon\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xepare\\PterodactylApiAddon\\PterodactylApiAddonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
